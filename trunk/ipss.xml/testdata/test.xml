				<ipss:lfMethod>NR</ipss:lfMethod>
				<ipss:maxIterations>20</ipss:maxIterations>
				<ipss:tolerance>0.0001</ipss:tolerance>
				<ipss:simuConfig>
					<ipss:totalSimuTimeSec>5.0</ipss:totalSimuTimeSec>
					<ipss:simuStepSec>0.002</ipss:simuStepSec>
					<ipss:simuMethod>ModifiedEuler</ipss:simuMethod>
					<ipss:absoluteMachAngValue>false</ipss:absoluteMachAngValue>
					<ipss:refMachineBusId>Mach@0005</ipss:refMachineBusId>
				</ipss:simuConfig>
				<ipss:dynamicEventData>
					<ipss:disableEvent>false</ipss:disableEvent>
					<ipss:eventList>
						<ipss:event>
							<ipss:name>Fault@0001</ipss:name>
							<ipss:eventType>Fault</ipss:eventType>
							<ipss:startTimeSec>0.0</ipss:startTimeSec>
							<ipss:durationSec>0.1</ipss:durationSec>
							<ipss:fault>
								<ipss:faultType>BusFault</ipss:faultType>
								<ipss:faultCategory>Fault_3P</ipss:faultCategory>
								<ipss:busBranchId>0001</ipss:busBranchId>
							</ipss:fault>
						</ipss:event>
					</ipss:eventList>
				</ipss:dynamicEventData>
				<ipss:outputConfig>
					<ipss:outputFilter>true</ipss:outputFilter>
					<ipss:outputVarList>
						<ipss:variableName>Machine - Mach@0004</ipss:variableName>
						<ipss:variableName>Bus - 0001</ipss:variableName>
					</ipss:outputVarList>
				</ipss:outputConfig>
			    </ipss:dStabStudyCase>
			</ipss:dStabStudyCaseRec>

