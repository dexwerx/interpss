<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
         xmlns:p="http://www.springframework.org/schema/p"
	     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="aclfSummaryReport" 
	      class="org.interpss.report.impl.AclfSummaryReport"
	      scope="singleton">	
		<property name="fnameAclfSummaryMaster">
	    	<value>reportTemplate/aclf/AclfSummaryMaster.jasper</value></property>
		<property name="fnameSubAclfMismatch">
	    	<value>reportTemplate/aclf/AclfMismatchSubReport.jasper</value></property>
		<property name="fnameSubAclfPVLimit">
	    	<value>reportTemplate/aclf/AclfPVLimitSubReport.jasper</value></property>
		<property name="fnameSubAclfPQLimit">
	    	<value>reportTemplate/aclf/AclfPQLimitSubReport.jasper</value></property>
		<property name="fnameSubAclfFuncLoad">
	    	<value>reportTemplate/aclf/AclfFuncLoadSubReport.jasper</value></property>
		<property name="fnameSubAclfRemoteQBus">
	    	<value>reportTemplate/aclf/AclfRemoteQBusSubReport.jasper</value></property>
		<property name="fnameSubAclfPSXfrPControl">
	    	<value>reportTemplate/aclf/AclfPSXfrPControlSubReport.jasper</value></property>
		<property name="fnameSubAclfTapVControl">
	    	<value>reportTemplate/aclf/AclfTapVControlSubReport.jasper</value></property>	
	 </bean>

	 <bean id="aclfBusStyleReport" 
	      class="org.interpss.report.impl.AclfBusStyleReport"
	      scope="singleton">	
		<property name="fnameAclfBusStyleMaster">
	    	<value>reportTemplate/aclf/AclfBusStyleMaster.jasper</value></property>
		<property name="fnameSubAclfMismatch">
	    	<value>reportTemplate/aclf/AclfMismatchSubReport.jasper</value></property>
		<property name="fnameSubAclfPVLimit">
	    	<value>reportTemplate/aclf/AclfPVLimitSubReport.jasper</value></property>
		<property name="fnameSubAclfPQLimit">
	    	<value>reportTemplate/aclf/AclfPQLimitSubReport.jasper</value></property>
		<property name="fnameSubAclfFuncLoad">
	    	<value>reportTemplate/aclf/AclfFuncLoadSubReport.jasper</value></property>
		<property name="fnameSubAclfRemoteQBus">
	    	<value>reportTemplate/aclf/AclfRemoteQBusSubReport.jasper</value></property>
		<property name="fnameSubAclfPSXfrPControl">
	    	<value>reportTemplate/aclf/AclfPSXfrPControlSubReport.jasper</value></property>
		<property name="fnameSubAclfTapVControl">
	    	<value>reportTemplate/aclf/AclfTapVControlSubReport.jasper</value></property>	
	 </bean>

<!--
   the only difference between 3P and NS fault is the master template:

		<property name="fnameAcscFaultMaster">
	    	<value>reportTemplate/acsc/Acsc3PFaultMaster.jasper</value></property>	

		<property name="fnameAcscFaultMaster">
	    	<value>reportTemplate/acsc/AcscNSFaultMaster.jasper</value></property>	
-->

	 <bean id="acsc3PFaultReport" 
	      class="org.interpss.report.impl.AcscFaultReport"
	      scope="singleton">	
		<property name="fnameAcscFaultMaster">
	    	<value>reportTemplate/acsc/Acsc3PFaultMaster.jasper</value></property>	
		<property name="fnameSubFaultSummary">
	    	<value>reportTemplate/acsc/AcscFaultSummarySubReport.jasper</value></property>	
	 </bean>

	 <bean id="acscNSFaultReport" 
	      class="org.interpss.report.impl.AcscFaultReport"
	      scope="singleton">	
		<property name="fnameAcscFaultMaster">
	    	<value>reportTemplate/acsc/AcscNSFaultMaster.jasper</value></property>	
		<property name="fnameSubFaultSummary">
	    	<value>reportTemplate/acsc/AcscFaultSummarySubReport.jasper</value></property>	
	 </bean>
</beans>
