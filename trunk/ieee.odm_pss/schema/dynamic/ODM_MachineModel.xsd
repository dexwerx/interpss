			<element name="qContribution" type="double" maxOccurs="1" minOccurs="0"></element>
		</sequence>
	</complexType>

	<simpleType name="GeneratorEmumType">
		<restriction base="string">
			<enumeration value="equi_gen_unit"></enumeration>
			<enumeration value="classicalModel"></enumeration>
			<enumeration value="transientModel"></enumeration>
			<enumeration value="subtransModel"></enumeration>
		</restriction>
	</simpleType>

