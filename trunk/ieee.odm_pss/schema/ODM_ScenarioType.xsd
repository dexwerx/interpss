							<simpleType>
								<restriction base="string">
									<enumeration value="busFault"></enumeration>
									<enumeration value="branchFault"></enumeration>
									<enumeration value="branchOutage"></enumeration>
									<enumeration value="loadChange"></enumeration>
								</restriction>
							</simpleType>
						</element>

						<element name="branchFault" maxOccurs="1" minOccurs="0">
