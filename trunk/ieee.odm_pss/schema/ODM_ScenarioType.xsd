								</sequence>
							</complexType>
						</element>
						<element name="busFault" maxOccurs="1" minOccurs="0">
							<complexType>
								<sequence>
									<element name="faultCategory" type="pss:FaultCategoryXmlType" maxOccurs="1" minOccurs="1"></element>
									<element name="faultedBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="1"></element>
									<element name="faultedBusRatedV" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></element>
									<element name="remoteEndBus" type="pss:IDRefRecordXmlType" maxOccurs="1" minOccurs="0"></element>
									<element name="remoteEndBusRatedV" type="pss:VoltageXmlType" maxOccurs="1" minOccurs="0"></element>


									<element name="faultStartTime" type="pss:CycleXmlType" maxOccurs="1" minOccurs="0"></element>
									<element name="firstOperationTime" type="pss:CycleXmlType" maxOccurs="1" minOccurs="0"></element>
									<element name="secondOperationTime" type="pss:CycleXmlType" maxOccurs="1" minOccurs="0"></element>
									<element name="faultDurationTime" type="pss:CycleXmlType" maxOccurs="1" minOccurs="0"></element>
